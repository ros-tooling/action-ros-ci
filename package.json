{
	"name": "action-ros-ci",
	"version": "0.2.1",
	"description": "GitHub Action compiling and testing a ROS 2 package",
	"main": "lib/main.js",
	"scripts": {
		"build": "ncc build src/action-ros-ci.ts -o dist",
		"fixup": "eslint . --fix",
		"lint": "eslint .",
		"test": "tsc --noEmit && jest --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ros-tooling/action-ros-ci.git"
	},
	"keywords": [
		"actions",
		"ros",
		"ci"
	],
	"author": "ROS 2 Tooling Working Group",
	"license": "Apache-2.0",
	"licenses": [
		{
			"type": "Apache-2.0",
			"url": "http://www.apache.org/licenses/LICENSE-2.0"
		}
	],
	"bugs": {
		"url": "https://github.com/ros-tooling/action-ros-ci/issues"
	},
	"dependencies": {
		"@actions/core": "^1.4.0",
		"@actions/exec": "^1.0.4",
		"@actions/github": "^4.0.0",
		"@actions/io": "^1.1.0",
		"async-retry": "^1.3.1"
	},
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"@types/node": "^15.12.2",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.24.0",
		"@vercel/ncc": "^0.27.0",
		"acorn": "^8.1.0",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"jest-circus": "^27.0.1",
		"lint-staged": "^10.5.4",
		"prettier": "2.3.0",
		"ts-jest": "^26.5.6",
		"typescript": "^4.3.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
